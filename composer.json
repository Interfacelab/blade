{
  "name": "ilab/blade",
  "type": "library",
  "description": "Use Laravel Blade templates without the full Laravel framework",
  "keywords": [
    "laravel",
    "templating",
    "template",
    "views"
  ],
  "homepage": "https://github.com/ilab/blade",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Craig Duncan",
      "email": "git@duncanc.co.uk",
      "homepage": "https://github.com/duncan3dc",
      "role": "Developer"
    }
  ],
  "require": {
    "ilab/illuminate-contracts": ">=5.5,<5.6",
    "ilab/illuminate-events": ">=5.5,<5.6",
    "ilab/illuminate-filesystem": ">=5.5,<5.6",
    "ilab/illuminate-view": ">=5.5,<5.6",
    "php": "^7.0"
  },
  "require-dev": {
    "duncan3dc/object-intruder": "^0.1.0",
    "mockery/mockery": "^0.9.5",
    "phpunit/phpunit": "^6.0",
    "satooshi/php-coveralls": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "ILAB\\Laravel\\": "src/",
      "ILAB\\LaravelTests\\": "tests/"
    }
  },
  
  "repositories": [{
    "type": "composer",
    "url": "https://packages.interfacelab.io"
  }]
}